@import '../../../component/demon/demon.wxss';
page {
    width: 100%;
    height: 100%;
    background-color: rgb(243, 243, 243);
}
.company-container{
    width: 100%;
    height: 100%;
}
.header{
    display: flex;
    flex-direction: column;
}
.header-img{
    width: 100%;
    padding-top: 70%;
    position: relative;
    background-color: paleturquoise;
}
.header-img image {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.header-content{
    padding: 38rpx 45rpx;
    background-color: #fff;
}
.header-title{
    text-align: center;
    font-size: 30rpx;
}
.header-text{
    color: rgb(141, 141, 141);
    font-size: 28rpx;
    display: inline-block;
    height: 40rpx;
    overflow: hidden;
    text-overflow: ellipsis;
}
.header-text-more{
    color: rgb(141, 141, 141);
    font-size: 28rpx;
    display: inline-block;
    overflow: hidden;
    text-overflow: ellipsis;
}
.header-more{
    color: rgb(73, 159,228);
    font-size: 24rpx;
    text-align: center;
}
.bindtap{
    display: inline-block
}
.header-icon{
    background-image: url(data:image/png;base64,);
    width: 19rpx;
    height: 20rpx;
    display: inline-block;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}
.header-icon-rotate{
    transform: rotate(180deg)
}
.c-container{
    background-color: #fff;
    margin-top: 20rpx;
}
.c-tab{
    display: flex;
    font-size: 30rpx;
    border-bottom: 1px solid #ddd;
}
.c-tab-title{
    flex: 1;
    text-align: center;
    line-height: 60rpx;
}
.c-active{
    background-color: rgb(239, 247, 253)
}
.c-lianx{
    padding: 0 25rpx;
    font-size: 28rpx;
    color: rgb(189, 189, 189)
}
.c-callme{
    width: 100%;
}
.c-lianx-header{
    margin-top: 15rpx;
}
.c-collab-wrapper{
    display: flex;
    flex-wrap: wrap;
    padding: 0 15rpx;
}
.c-collab{
    width: 50%;
}
.c-demon-item{
    padding: 30rpx
}