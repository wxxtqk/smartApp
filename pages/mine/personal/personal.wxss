.personal-container{
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgb(239, 239, 244)
}
.personal-wrapper{
    margin-top: 30rpx;
    background-color: #ffffff;
    box-sizing: border-box;
    border-top: 1rpx solid #dddddd;
    border-bottom: 1rpx solid #dddddd;
    padding:5rpx 0 0 20rpx;
}
.personal-item{
    margin-top: 5rpx;
    border-bottom: 1rpx solid #dddddd;
    display: flex;
    height: 100rpx;
    line-height: 100rpx;
    align-items: center;
}
.personal-item-l{
    display: inline-block;
    width: 200rpx;
}
.personal-item-r{
    flex: 1;
    margin-left: 10rpx;
    height: 100%;
}
.personal-item-r image{
  width: 60rpx;
  height: 60rpx;
  vertical-align:middle;
  line-height: 0;
}
.personal-btn-area{
    width: 90%;
    margin: 0 auto;
    margin-top: 30rpx;
    background-color: rgb(82, 162, 233);
    border-radius: 10rpx;
    box-shadow: 0px 0 8px rgb(82, 162, 233);
    margin-bottom: 50rpx;
}
.personal-btn{
    background-color: rgb(82, 162, 233)
}