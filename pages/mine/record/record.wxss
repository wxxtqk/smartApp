page{
    height: 100%;
    background-color:rgb(239, 239, 244);
}
.favorite-container{
    /* position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0; */
    background-color: rgb(239, 239, 244);
    display: flex;
    flex-direction: column;
    padding: 0 20rpx;
    box-sizing: border-box;
    /* height: 100%; */
    overflow-y: auto;
}
.favorite-list{
    background-color: #ffffff;
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-top: 19rpx;
    padding: 24rpx 22rpx;
    box-sizing: border-box;
    font-size: 32rpx;
}
.favorite-img{
    width:181rpx;
    height:146rpx;
    border-radius: 6rpx;
}
.favorite-item{
    display: flex;
    flex-direction: column;
    height:146rpx;
}
.favorite-desc{
    width: 100%;
    flex: 1;
}
.favorite-date{
    height: 46rpx;
    line-height: 46rpx;
    font-size: 26rpx;
}
.img-wrapper{
    position: relative;
    line-height: 0;
    margin-right: 14rpx;
}
.voice{
    position: absolute;
    right: 0;
    bottom: 0;
    width:30rpx;
    height:30rpx;
    background-image: url(data:image/png;base64,)
}