.plan-list {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  flex-direction: column;
  padding: 0 20rpx;
  background-color: rgb(239, 239, 244);
}
.wait-study {
  height: 100rpx;
  width: 100%;
  text-align: left;
  font-family: sourceSansCn-Bold;
  font-size: 26rpx;
  color: rgb(73, 159, 228);
  line-height: 100rpx;
}
.curriculum-plan {
  width: 100%;
}
.curriculum {
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  margin-bottom: 19rpx;
  border-radius: 6rpx;
}
.flex-wrp {
  display: flex; 
  justify-content: space-between; 
}
/* .flex-item{
   justify-content: space-between; 
   flex: 1; 
} */
.curriculum-package {
  padding: 24rpx 22rpx;
}
.curriculum-title-package {
  padding:0 14rpx 0 23rpx;
}
.curriculum-image {
  width:181rpx;
  height:146rpx;
  border-radius: 6rpx;
  background-color: #f4f4f4;
}
/* 音频课程图标 */
.voice{
    position: absolute;
    right: 0;
    bottom: 0;
    width:30rpx;
    height:30rpx;
    background-image: url(data:image/png;base64,)
}
.curriculum-title {
  max-width: 364rpx;
  font-size:32rpx;
  display:block;
  width: 400rpx;
  /* max-height:80rpx; */
  /* overflow:scroll; */
}
.curriculum-name {
  color: rgb(141, 141, 141);
  font-size: 28rpx;
  display:block;
}
.goStudy {
  position: relative;
  top: 25%;
  width: 114rpx;
  height: 50rpx;
  border: 2px solid rgb(30, 159, 239);
  border-radius: 6rpx;
  color: rgb(30, 159, 239);
  font-size: 22rpx;
  text-align:center;
  line-height: 50rpx;
}
.planStudyProgress {
  width: 80rpx;
  height: 80rpx;
  border: 3px solid rgb(8,151,239);
  border-radius: 80rpx;
  color: rgb(141, 141, 141);
  text-align: center;
  line-height: 80rpx;
  font-size:28rpx;
}
.planStudyProgress-title {
  color: rgb(141, 141, 141);
  font-size: 28rpx;
  display:block;
  padding-top: 20rpx;
  text-align: center;
}
/* 2rpx = 1px  */